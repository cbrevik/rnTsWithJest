{
  "name": "rnTsWithJest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.47.8",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.0.1",
    "jest": "20.0.4",
    "react-native-typescript-transformer": "^1.1.3",
    "react-test-renderer": "16.0.0-alpha.12",
    "ts-jest": "^20.0.10",
    "typescript": "^2.4.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "ts-jest.tsconfig.json"
      }
    },
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"]
  }
}
